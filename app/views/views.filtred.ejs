<!doctype html>
<html lang="en">

<head>
    <%- include('./partials/head', {title:'Dash-reports'}) %>
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <script>
        const logOut = () => {
            sessionStorage.clear();
            window.location.href= "/"
        };
    </script>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-2">
                <%- include('./partials/header-dash') %>
            </div>
            <div class="col-sm-10 contentDelete">
                <div class="container mt-4">
                    <div class="row">
                        <span class="col-10 title-user">Reportes</span>
                        <div class="col-2 p-1 sidebar-item"><i class="sidebar-icon bi bi-person-circle"></i> <span
                                style="margin-left: 10px;">Emmanuel</span></div>
                    </div>
                </div>
                <div class="modal fade" id="exampleAddUserModal" tabindex="-1"
                    aria-labelledby="exampleEditUserModalLabel" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="modal-title fs-5" id="exampleEditUserModalLabel">Agregar Usuarios</h3>
                                <button type="button" class="btn btn-outline-dark bi-x btn-close-modal"
                                    data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <div class="modal-body">
                                <form>
                                    <div class="mb-3">
                                        <label for="nombre" class="col-form-label">Nombre</label>
                                        <input type="text" class="form-control alert-dark rounded nombre" id="nombre">
                                    </div>
                                    <div class="mb-3">
                                        <label for="celular" class="col-form-label">Celular</label>
                                        <input type="text" class="form-control alert-dark rounded celular" id="celular">
                                    </div>
                                    <div class="mb-3">
                                        <label for="cedula" class="col-form-label">Cedula</label>
                                        <input type="text" class="form-control alert-dark rounded cedula" id="cedula">
                                    </div>
                                    <div class="mb-3">
                                        <label for="direccion" class="col-form-label">Direccion</label>
                                        <input type="text" class="form-control alert-dark rounded direccion"
                                            id="direccion">
                                    </div>
                                    <div class="mb-3">
                                        <label for="correo" class="col-form-label">Correo</label>
                                        <input type="text" class="form-control alert-dark rounded correo" id="correo">
                                    </div>
                                    <div class="mb-3">
                                        <label for="contrasena" class="col-form-label">Contraseña</label>
                                        <input type="text" class="form-control alert-dark rounded contrasena"
                                            id="contrasena">
                                    </div>
                                    <div class="mb-3">
                                        <label for="contrasena" class="col-form-label">Id Rol</label>
                                        <input type="text" class="form-control alert-dark rounded rol" id="contrasena">
                                    </div>
                                </form>
                                <div style="text-align: center;">
                                    <button type="button" class="btn btn-warning btn-save">Guardar</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Modal Editar Usuario -->
                <div class="modal fade" id="exampleEditUserModal" tabindex="-1"
                    aria-labelledby="exampleEditUserModalLabel" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="modal-title fs-5" id="exampleEditUserModalLabel">Editar Usuarios</h3>
                                <button type="button" class="btn btn-outline-dark bi-x btn-close-modal"
                                    data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <div class="modal-body">
                                <form>
                                    <div class="mb-3">
                                        <label for="nombreEdit" class="col-form-label">Nombre</label>
                                        <input type="text" class="form-control alert-dark rounded nombreEdit"
                                            id="nombreEdit">
                                    </div>
                                    <div class="mb-3">
                                        <label for="celularEdit" class="col-form-label">Celular</label>
                                        <input type="text" class="form-control alert-dark rounded celularEdit"
                                            id="celularEdit">
                                    </div>
                                    <div class="mb-3">
                                        <label for="cedulaEdit" class="col-form-label">Cedula</label>
                                        <input type="text" class="form-control alert-dark rounded cedulaEdit"
                                            id="cedulaEdit">
                                    </div>
                                    <div class="mb-3">
                                        <label for="direccionEdit" class="col-form-label">Direccion</label>
                                        <input type="text" class="form-control alert-dark rounded direccionEdit"
                                            id="direccionEdit">
                                    </div>
                                    <div class="mb-3">
                                        <label for="correoEdit" class="col-form-label">Correo</label>
                                        <input type="text" class="form-control alert-dark rounded correoEdit"
                                            id="correoEdit">
                                    </div>
                                    <div class="mb-3">
                                        <label for="contrasenaEdit" class="col-form-label">Contraseña</label>
                                        <input type="text" class="form-control alert-dark rounded contrasenaEdit"
                                            id="contrasenaEdit">
                                    </div>
                                </form>
                                <div style="text-align: center;">
                                    <button type="button" class="btn btn-warning btn-save-edit"
                                        data-bs-dismiss="modal">Guardar</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Modal Eliminar Usuario -->
                <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false"
                    tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <div class="modal-header" style="align-self: center;padding-bottom:20px">
                                <h5 class="modal-title fs-5" id="staticBackdropLabel">¿Deseas eliminar a "Esneider"?
                                </h5>
                            </div>
                            <div style="text-align: center;padding-bottom: 20px;">
                                <button type="button" class="btn btn-warning accept" data-bs-dismiss="modal"
                                    style="color:white">Aceptar</button>
                                <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Rechazar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="container">
                    <div class="row align-items-start mobile-table-report">
                        <div class="col mt-5" style="text-align-last: center;text-align: -webkit-center;">
                            <span class="col-10 title-user" style="padding-left: 0;">Reportes de Ventas Generales</span>
                            <img width="55%" height="auto" class="img-report"
                                src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fmexico.unir.net%2Fwp-content%2Fuploads%2Fsites%2F6%2F2022%2F05%2Fgrafico-de-barras.jpg&f=1&nofb=1&ipt=528046bceeecbd19ceee01042f4a2f651e02895a65cc6b9ab2cfb90004fb3e13&ipo=images">
                            <div class="form-group col-md-4 mt-4">
                                <select id="inputState" class="form-control">
                                    <option selected disabled>Cadena</option>
                                    <option>Éxito</option>
                                    <option>Carulla</option>
                                    <option>Metro</option>
                                    <option>D1</option>
                                </select>
                            </div>
                        </div>
                        <div class="col" style="text-align: -webkit-center;">
                            <!-- Barra de búsqueda -->
                            <div class="container mt-5">
                                <div class="search-and-add">
                                    <!-- Barra de búsqueda -->
                                    <div class="search-bar">
                                        <div class="input-group" style="width: 100%;">
                                            <!-- <input type="search" class="form-control rounded-pill alert-dark" aria-label="Buscar usuario"><i class="bi bi-search"></i> -->
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="ventas">
                                <% if (detailsOrdersFiltred && detailsOrdersFiltred.length> 0) { %>
                                    <% detailsOrdersFiltred.forEach(detailOrder=> { %>
                                        <div class="row rounded shadow mt-3 img-product-report"
                                            style="width: 80%; align-items: center; border: 1px solid #000;">
                                            <img width="100" height="100" src="<%= detailOrder.imagen %>">
                                            <div class="" style="margin-left: 10px;">
                                                <div>
                                                    <span class="info-product p-1">
                                                        <%= detailOrder.NombreProducto %>
                                                    </span>
                                                    <span class="info-product p-1">Cantidad: <%= detailOrder.Cantidad %>
                                                            </span>
                                                </div>
                                                <div>
                                                    <span class="info-product p-1">Precio: <%= detailOrder.PrecioVenta
                                                            %></span>
                                                    <a href="ReportesIndividuales.html"><span
                                                            style="text-decoration: underline;"
                                                            class="info-product p-1">Cadena: <%=
                                                                detailOrder.nombre_proveedor %></span></a>
                                                </div>
                                            </div>
                                        </div>
                                        <% }); %>
                                            <% } else { %>
                                                <p>No se encontraron detalles de pedidos.</p>
                                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/details.js"></script>
    <%- include('./partials/scripts') %>
</body>
</html>